{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  },
  "rewrites": [
    { "source": "/api/track-event", "destination": "/api/track-event.js" },
    { "source": "/api/analytics", "destination": "/api/analytics.js" },
    { "source": "/dashboard", "destination": "/dashboard/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
